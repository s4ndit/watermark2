{
  "name": "wasserzeichen-webapp",
  "version": "1.0.0",
  "description": "Moderne Webapp fÃ¼r Wasserzeichen auf Bilder und Videos mit FFmpeg und ImageMagick",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon server/index.js",
    "client:dev": "cd client && npm start",
    "build": "cd client && npm run build",
    "install:all": "npm install && cd client && npm install",
    "docker:build": "docker build -t wasserzeichen-webapp .",
    "docker:run": "docker run -p 3000:3000 -p 3001:3001 wasserzeichen-webapp"
  },
  "dependencies": {
    "express": "^4.18.2",
    "socket.io": "^4.7.2",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "uuid": "^9.0.0",
    "sharp": "^0.32.5",
    "fluent-ffmpeg": "^2.1.2",
    "child_process": "^1.0.2",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "mime-types": "^2.1.35"
  },
  "devDependencies": {
    "nodemon": "^3.0.1",
    "concurrently": "^8.2.0"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "author": "Wasserzeichen Webapp",
  "license": "MIT"
} 